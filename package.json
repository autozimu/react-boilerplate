{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate for ReactJS project",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "tslint --fix --project tsconfig.json",
    "prettier": "prettier --write 'src/**/*.ts*' 'webpack.config*' tsconfig.json tslint.json",
    "build": "webpack",
    "release": "tsc && npm run build && npm run prettier && npm run lint",
    "clean": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/autozimu/react-boilerplate.git"
  },
  "keywords": [],
  "author": "Junfeng Li <autozimu@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.80.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^6.0.0",
    "awesome-typescript-loader": "^5.2.0",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.2.0",
    "parcel-bundler": "^1.8.1",
    "prettier": "^1.12.1",
    "react-hot-loader": "^4.3.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3",
    "webpack": "^4.13.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
