{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate for ReactJS project",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "tslint --project tsconfig.json",
    "prettier": "prettier --write 'src/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.config.production.js",
    "postbuild": "copyfiles index.html dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/autozimu/react-boilerplate.git"
  },
  "keywords": [],
  "author": "Junfeng Li <autozimu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-redux": "^5.0.8",
    "@types/redux": "^3.6.0",
    "awesome-typescript-loader": "^3.2.3",
    "copyfiles": "^1.2.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.28.1",
    "prettier": "^1.12.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.0",
    "style-loader": "^0.17.0",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.5.2",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.7.2",
    "semantic-ui-react": "^0.68.3"
  }
}
